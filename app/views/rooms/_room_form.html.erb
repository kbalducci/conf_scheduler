
<h1>Schedule a Conference Room</h1>
<%= simple_form_for @room do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :room_name %>
    <%= f.input :location %>
    <%= f.input :description %>
    <%= f.input :photo %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>

<% end %>

