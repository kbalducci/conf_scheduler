<div class="row">
  <div class="col-md-4">
    <div class="well well-lg">
      <h3 style="text-align: center">Add a Conference Room</h3>
    </div>
  <%= render 'room_form' %>
  </div>
  <div class="col-md-1">
  </div>
  <div class="col-md-6">
    <div class="well well-lg">
      <h2 style="text-align: center">Our Conference Rooms</h2>
    </div>
      <% @rooms.each do |room| %>
      <ul>
        <li class="room-info"><h3><%= link_to room.room_name, room_path(room) %></h3></li>
        <li class="room-info"><%= image_tag room.photo.url(:medium) %></li>
        <li class="room-info">Location: <%= room.location %></li>
        <li class="room-info">Description: <%= room.description %></li>
      </ul>
      <hr />
          <% end %>
      <%= paginate @rooms %><br />
      <%= page_entries_info @rooms %>
  </div>
  <div class="col-md-1">
  </div>
</div>



