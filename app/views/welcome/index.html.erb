<h1>Welcome Index</h1>
<% if user_signed_in? %>

        <p><%= link_to('Logout', destroy_user_session_path(@current_user), :method => :delete, class: "btn btn-primary btn-lg" ) %></p>

      <% else %>
        <p><%= link_to('Sign Up Now', new_user_registration_path, class: "btn btn-primary btn-lg")%></p>
        <p><%= link_to('Sign In Now', new_user_session_path, class: "btn btn-primary btn-lg")%></p>

      <% end %>
