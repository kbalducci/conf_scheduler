<p id="notice"> <%= notice %></p>
<div class="row">
  <div class="col-md-3">
  </div>
  <div class="col-md-6">
    <h1><%= @user.full_name %></h1>
      <ul>
        <li class="text-success">Date of Birth: <%= @user.dob %></li>
        <li class="text-success">Favorite Color: <%= @user.fav_color %></li>
      </ul>
    <table id='meetings-table' class="table table-striped table-hover ">
      <thead>
        <tr>
          <th>Meeting</th>
          <th>Start Time</th>
          <th>End Time</th>
        <!--   <th>Comment</th> -->
        </tr>
      </thead>
      <tbody>
      <% @user.meetings.each do |meeting| %>
        <tr>

          <td><%= meeting.title %></td>
          <td><time><%= meeting.start_time %></time></td>
          <td><time><%= meeting.end_time %></time></td>
         <!-- meeting.comment.message -->
          <% end %>
        </tr>
      </tbody>
    </table>
  </div>
</div>



