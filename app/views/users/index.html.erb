<div class="row">
  <div class="col-md-3">
  </div>
  <div class="col-md-6">
    <h1>Our Employees</h1>
    <h3>Please click on an employee's name to go to their profile page</h3>
    <table class="table table-striped table-hover ">
      <thead>
        <tr>
          <th>Name</th>
        </tr>
      </thead>
      <tbody>
      <% @users.each do |user| %>
        <tr>

          <td><%= link_to user.full_name, user_path(user) %></td>
          <% end %>
        </tr>
      </tbody>
    </table>
  </div>
  <div class="col-md-3">
  </div>
</div>

