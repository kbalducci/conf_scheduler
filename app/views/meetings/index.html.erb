<div class="container">
  <div class="row">
    <div class="col-md-4">
      <div class="well well-lg">
        <h3 style="text-align: center">Schedule a Meeting</h3>
      </div>
    <%= render 'meeting_form' %>
    </div>
    <div class="col-md-1">
    </div>
    <div class="col-md-6">
      <div class="well well-lg">
        <h2 style="text-align: center">Scheduled Meetings  </h2>
      </div>
<table class="table table-striped table-hover ">
  <thead>
    <tr>
      <th>Meeting</th>
<!--       <th>Location</th> -->
      <th>Start Time</th>
      <th>End Time</th>
    </tr>
  </thead>
  <tbody>
  <% @meetings.each do |meeting| %>
    <tr>
      <td><%= meeting.title %></td>
      <td><%= meeting.start_time %></td>
      <td><%= meeting.end_time %></td>

      <% end %>
    </tr>
  </tbody>
</table>

    <div class="col-md-1">
    </div>
  </div>
</div>
